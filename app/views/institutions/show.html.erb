<div class="col-lg-12">
  <h2><%= @institution.name %></h2>
  <div class="row">
    <div class="well">
		  Description: <%= @institution.description %><br />
		  Contact Name: <%= @institution.contact_name %><br />
		  Email: <%= @institution.email %><br />
		  Phone: <%= @institution.phone %><br />
		  Address 1: <%= @institution.address_1 %><br />
		  Address 2: <%= @institution.address_2 %><br />
		  City: <%= @institution.city %><br />
		  State: <%= @institution.state %><br />
		  Postal Code: <%= @institution.postal_code %><br />
		  Country: <%= @institution.country %><br />
		  Website: <%= @institution.website %><br />
			Company Type: <%= @institution.company_type %><br />
    </div>
  </div>
</div>
